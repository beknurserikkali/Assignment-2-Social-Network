package base;

class Post1 extends Post {
    private String username;
    private String content;
    public Post1(String username, String content) {
        super(username, content);
        this.username = username;
        this.content = content;
    }

    @Override
    public String toString() {
        return "Post1{" +
                "username='" + username + '\'' +
                ", content='" + content + '\'' +
                '}';
    }
}
